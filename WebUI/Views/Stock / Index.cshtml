@model List<WebUI.Models.StockItem>

<h2>Остатки на складе</h2>

<table border="1">
    <tr>
        <th>ID</th>
        <th>Название</th>
        <th>Количество</th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>@item.Id</td>
        <td>@item.ProductName</td>
        <td>@item.Quantity</td>
    </tr>
}
</table>
